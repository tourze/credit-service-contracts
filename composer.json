{
  "name": "tourze/credit-service-contracts",
  "description": "积分服务定义包，包含接口、异常和枚举类定义",
  "type": "library",
  "license": "MIT",
  "require": {
    "symfony/security-core": "^7.3",
    "tourze/enum-extra": "0.1.*",
    "tourze/user-id-bundle": "0.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-enum": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\CreditServiceContracts\\": "src/"
